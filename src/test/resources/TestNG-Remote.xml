<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression" thread-count="1" parallel="tests">
    <test name="Chrome">
        <parameter name="environment"        value="uat"/>
        <parameter name="test.configuration" value="chromeremote.properties"/>
        <parameter name="run.environment"    value="grid"/>
        <classes>
            <class name="starter.testing.ExecutionEngine"/>
        </classes>
    </test>
    <test name="Firefox">
        <parameter name="environment"        value="uat"/>
        <parameter name="test.configuration" value="firefoxremote.properties"/>
        <parameter name="run.environment"    value="grid"/>
        <classes>
            <class name="starter.testing.ExecutionEngine"/>
        </classes>
    </test>
</suite>


